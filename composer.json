{
    "name": "tlm/laravel-axepta",
    "description": "Laravel package for BNP Paribas Axepta payment integration",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "axepta",
        "bnp-paribas",
        "payment",
        "gateway"
    ],
    "authors": [
        {
            "name": "Tahirou LAOUAN MAGAGI",
            "email": "lmtahirou@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "phpseclib/phpseclib": "^3.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0|^9.0",
        "phpunit/phpunit": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "TLM\\LaravelAxepta\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TLM\\LaravelAxepta\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TLM\\LaravelAxepta\\AxeptaServiceProvider"
            ],
            "aliases": {
                "Axepta": "TLM\\LaravelAxepta\\Facades\\Axepta"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}